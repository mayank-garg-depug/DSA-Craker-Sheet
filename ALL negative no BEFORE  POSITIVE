int main() {
    int a[]={-12, 11, -13, -5, 6, -7, 5, -3, -6 };
    int j=0,n=sizeof(a)/sizeof(a[0]);
    for(int i=0;i<n;i++)
    {
        if(a[i]<0)
        {
         //   if(i!=j)
            //{
                swap(a[i],a[j]);
                j++;
            //}
            
        }
    }
     for(int i=0;i<n;i++)
     cout<<a[i]<<" ";
	return 0;
}
